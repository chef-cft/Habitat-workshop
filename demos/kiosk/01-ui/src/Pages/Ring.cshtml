@page
<style>.json {font-size: smaller }</style>

<div class="breadcrumb-option">
    <div class="container">
        <div class="row">
            <div class="col-lg-6 col-md-6 col-sm-6">
                <div class="breadcrumb__text">
                    <h2>Ring Details</h2>
                </div>
            </div>
            <div class="col-lg-6 col-md-6 col-sm-6">
                <div class="breadcrumb__links">
                    <a href="/">Home</a>
                    <span>Habitat Ring</span>
                </div>
            </div>
        </div>
    </div>
</div>


<section class="shop spad">
